resource "google_compute_network" "{{vpc_name}}" {
  name = "{{vpc_name}}-{{env}}-vpc"
  auto_create_subnetworks = "false"
}

resource "google_compute_subnetwork" "{{subnet_name}}" {
  name   = "{{subnet_name}}-{{env}}-subnet"
  ip_cidr_range = "{{ip_cidr_range}}"
  region = "{{region}}"
  network = "${google_compute_network.{{vpc_name}}.self_link}"
  private_ip_google_access = "{{private_ip_google_access}}"
}
